<Window x:Class="GraphIN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:viewModels="clr-namespace:GraphIN"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="600">


    <Grid>
        <lvc:CartesianChart Margin="0,0,391,92">
            <lvc:CartesianChart.Series>
                <lvc:LineSeries Values="{Binding Values}" />
            </lvc:CartesianChart.Series>
        </lvc:CartesianChart>
        <Button Content="Button" HorizontalAlignment="Left" Margin="456,343,0,0" VerticalAlignment="Top" Click="Button_Click" Width="111"/>
        <TextBox x:Name="Texty" HorizontalAlignment="Left" Margin="306,37,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="253" Height="234"/>
    </Grid>
</Window>
